@model ProyectoFinal_MedLife.Models.Perfil

<form class= "formInicioSesion" method="post" asp-action="Login" asp-controller="Cuenta">
    <h1 class="h3 mb-3">Inicio de sesion</h1>
    <div class="form-floating">
        <input type="text" class="form-control" required asp-for="Email" />
        <label for="floatingInput">Gmail</label>
    </div>
    <div class="form-floating">
        <input type="password" class="form-control" required asp-for="Contrasena" />
        <label for="floatingPassword">Contrase√±a</label>
    </div>
    <div class=" checkbox mb-3">
        <label>
            <input type="checkbox" asp-for="MantenerActivo" /> Recordar sesion
        </label>
    </div>
    <button type="submit" class="btn-lg btn-primary">Ingresar</button>
</form>
<label class="alert-danger">@ViewBag.Error</label>
